  shorttitle = {On {{LoRaWAN}} Scalability},
  booktitle = {2017 {{European Conference}} on {{Networks}} and {{Communications}} ({{EuCNC}})},
  author = {Mikhaylov, Konstantin and Petajajarvi, Juha and Janhunen, Janne},
