  booktitle = {2017 {{IEEE}} 28th {{Annual International Symposium}} on {{Personal}}, {{Indoor}}, and {{Mobile Radio Communications}} ({{PIMRC}})},
  author = {Bankov, Dmitry and Khorov, Evgeny and Lyakhov, Andrey},
