  file = {Robyns et al. - 2017 - Physical-layer fingerprinting of LoRa devices usin.pdf:/home/aghiles/Aghiles/tool/tools/r-bibliography/storage/2BN39DVF/Robyns et al. - 2017 - Physical-layer fingerprinting of LoRa devices usin.pdf:application/pdf},
  groups = {lora-new-3},
