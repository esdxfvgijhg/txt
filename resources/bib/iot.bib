@article{liao_multi-hop_2017,
  title = {Multi-{{Hop LoRa Networks Enabled}} by {{Concurrent Transmission}}},
  author = {Liao, Chun-Hao and Zhu, Guibing and Kuwabara, Daiki and Suzuki, Makoto and Morikawa, Hiroyuki},
