@inproceedings{bor_lora_2017,
  timestamp = {2019-11-20T09:47:41Z},
  langid = {english},
  location = {{Ottawa, ON}},
  title = {{{LoRa Transmission Parameter Selection}}},
  isbn = {978-1-5386-3991-7},
