@article{ayoub_internet_2019,
  timestamp = {2019-11-20T09:24:29Z},
  langid = {english},
  title = {Internet of {{Mobile Things}}: {{Overview}} of {{LoRaWAN}}, {{DASH7}}, and {{NB}}-{{IoT}} in {{LPWANs Standards}} and {{Supported Mobility}}},
