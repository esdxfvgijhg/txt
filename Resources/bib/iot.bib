@inproceedings{dix-matthews_lora_2018,
  timestamp = {2019-11-20T09:52:55Z},
  langid = {english},
  location = {{Shenzhen, China}},
  title = {{{LoRa Parameter Choice}} for {{Minimal Energy Usage}}},
  isbn = {978-1-4503-6048-7},
  url = {http://dl.acm.org/citation.cfm?doid=3277883.3277888},
  doi = {10.1145/3277883.3277888},
  abstract = {LoRa (Long Range) is a radio technology that facilitates low power communications over a large distance. The long range of communication means that a simple single hop transmission to a centralised gateway is su\dbend{}cient for most applications. To supply this performance the LoRa physical layer o\dbend{}ers a range of transmission parameters to tailor the capabilities for di\dbend{}erent applications. With over 6000 parameter combinations it is di\dbend{}cult to select the most e\dbend{}cient combination. Using experimental measurements and simulations we show how to \dbend{}nd a parameter set with minimal energy usage for a given situation. Using data from real world deployments we also show that using payload replication with data-aware compression can improve data delivery ratios by 16 to 25\% at no or minimal extra energy cost. For energy-sensitive applications low energy LoRa settings and forward error correction give better results than settings that maximise the packet reception ratio.},
